<h1>All Shelters<h1>
<% @shelters.each do |shelter|  %>
   <section class="grid-container" id= 'shelter-<%=shelter.id%>'>
      <p><%= link_to shelter.name, "/shelters/#{shelter.id}" %><p> 
      <%= link_to  "Update Shelter", "/shelters/#{shelter.id}/edit" %>
      <%= link_to  "Delete Shelter", "/shelters/#{shelter.id}", method: :delete %> 
   </section>
<% end %>
 <%= link_to  "Create New Shelter", new_shelter_path %>